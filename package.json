		"@tailwindcss/postcss": "^4.0.0",
		"@tailwindcss/vite": "^4.0.0",
		"@testing-library/svelte": "^5.2.7",
		"@types/node": "^22.12.0",
		"@types/qrcode": "^1.5.5",
		"@vitest/coverage-v8": "^3.1.1",
		"autoprefixer": "^10.4.20",
		"msw": "^2.7.3",
		"supertest": "^7.1.0",
